{
  "manifest_version": 3,
  "name": "Tabs auto close",
  "version": "2.0.0",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs"],
  "icons": {
    "16": "icons/broom16.png",
    "48": "icons/broom48.png",
    "128": "icons/broom128.png"
  }
}
